@InProceedings{DelayedReciprocity2008,
  Title                    = {A questionnaire-based study on delayed reciprocity in a P2P-tv system},
  Author                   = {Jenneke E. Fokker and Piet H. Westendorp and Johan A Pouwelse and Huib de Ridder},
  Booktitle                = {Proceeding of the 1st international conference on Designing interactive user experiences for TV and video (UXTV '08)},
  Year                     = {2008},
  Editor                   = {ACM},
  Publisher                = {ACM},

  Abstract                 = {Using peer-to-peer (P2P) technology for distributing video files to
many viewers simultaneously can only be successful if users
cooperate voluntarily and massively. The current BitTorrent
protocol enforces instantaneous bandwidth exchange within the
context of one video file. To better exploit available resources for
P2P-TV systems, such as our Tribler (www.tribler.org) software,
we explore ways to induce user cooperation flexible over time and
resources. The research is based on the social exchange theory
and is done in two steps. First, the impact of social distance
expressed in the degrees of separation (DoS) on the trust in
reciprocity is investigated. Second, the DoS are combined with
the kind of exchangeable resources typical for P2P-TV systems
(i.e. injections and bandwidth) to find the influence of these
variables on the accepted delay in reciprocity. The discussion will
focus on the relation between trust in reciprocity and the accepted
delay in reciprocity.},
