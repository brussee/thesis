@Article{telcom-policy-2008,
  Title                    = {Pirates and Samaritans: A decade of measurements on peer production and their implications for net neutrality and copyright},
  Author                   = {J.A. Pouwelse and P. Garbacki and D.H.J. Epema and H.J. Sips},
